<%- include('header') %>

  <h1>NASA Object Search Listing</h1>
  <!-- back button takes the user to the index page -->
  <button onclick="location.href='/'" type="button" class="btn btn-primary"><span class="glyphicon glyphicon-plus"></span>Back
    to Search</button></br>
  </br><div class="col-md-12">
    <table class='table table-hover table-responsive table-bordered'>
      <thead style="border:1px solid" class="alert alert-info">
        <th style="border:1px solid"><strong>Name</strong></th>
        <th style="border:1px solid"><strong>Image</strong></th>
        <th style="border:1px solid"><strong>People</strong></th>
        <th style="border:1px solid"><strong>Url</strong></th>
      </thead>
    </table>


    <% if (test) { %>
      <%- test %>
    <% } %>

    <% if (nasa) { %>
      <% nasa.collection.items.forEach(object => { %>
          <p><%- object.data[0].title %> </p>
        <% }); %>
    <% } %>

    <% if (userValue) { %>
      <h4>You searched for:</h4>
       <h5>Title: <%- userValue.title %> </h5>
      <h5> Description: <%- userValue.description %> </h5>
    <% } %>
  
  <%- include('footer') %>